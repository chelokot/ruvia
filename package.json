{
  "name": "ruvia",
  "version": "0.1.8",
  "private": true,
  "main": "index.js",
  "scripts": {
    "prestart": "npm run generate:styles",
    "start": "expo start",
    "preandroid": "npm run generate:styles",
    "android": "expo run:android",
    "preios": "npm run generate:styles",
    "ios": "expo run:ios",
    "preweb": "npm run generate:styles",
    "web": "expo start --web",
    "build:web": "expo export -p web",
    "test": "jest",
    "postinstall": "patch-package",
    "prepare": "husky install",
    "generate:styles": "node scripts/generate-styles.js",
    "images:webp": "node scripts/convert-to-webp.js",
    "expo:assets": "node scripts/generate-missing-expo-assets.js",
    "deploy:firebase": "npm run build:web && npx firebase deploy --only hosting",
    "serve:firebase": "npx firebase emulators:start --only hosting",
    "bump:android-version": "node scripts/bump-version-code.js"
  },
  "dependencies": {
    "@fal-ai/client": "^1.6.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "dotenv": "^16.4.5",
    "expo": "~53.0.22",
    "expo-auth-session": "~6.2.1",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-file-system": "~18.1.11",
    "expo-font": "^13.0.2",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.5",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "~14.2.0",
    "firebase": "^10.12.2",
    "firebase-admin": "^13.5.0",
    "hono": "^4.9.5",
    "mongodb": "^6.19.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-iap": "^14.0.1",
    "react-native-purchases": "^9.3.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "0.21.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.79",
    "babel-jest": "^29.7.0",
    "expo-build-properties": "~0.12.5",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.3",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.3.3"
  },
  "overrides": {
    "metro": "0.81.5",
    "metro-config": "0.81.5"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/backend/",
      "<rootDir>/.eas-inspect",
      "<rootDir>/.eas-inspect-pre"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/.eas-inspect",
      "<rootDir>/.eas-inspect-pre"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|expo-.*|@expo/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-native-reanimated)/)"
    ]
  }
}
