{
  "name": "ruvia",
  "version": "0.1.27",
  "private": true,
  "main": "index.js",
  "scripts": {
    "prestart": "npm run generate:styles",
    "start": "expo start",
    "preandroid": "npm run generate:styles",
    "android": "expo run:android",
    "preios": "npm run generate:styles",
    "ios": "expo run:ios",
    "preweb": "npm run generate:styles",
    "web": "expo start --web",
    "build:web": "expo export -p web",
    "build:android": "expo export -p android",
    "test": "jest",
    "postinstall": "patch-package",
    "prepare": "husky install",
    "generate:styles": "node scripts/generate-styles.js",
    "images:webp": "node scripts/convert-to-webp.js",
    "expo:assets": "node scripts/generate-missing-expo-assets.js",
    "deploy:firebase": "npm run build:web && npx firebase deploy --only hosting",
    "serve:firebase": "npx firebase emulators:start --only hosting",
    "bump:android-version": "node scripts/bump-version-code.js"
  },
  "dependencies": {
    "@fal-ai/client": "^1.6.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "dotenv": "^17.2.2",
    "expo": "^54.0.0-preview.16",
    "expo-auth-session": "~7.0.7",
    "expo-camera": "~17.0.6",
    "expo-constants": "~18.0.7",
    "expo-file-system": "~19.0.10",
    "expo-font": "~14.0.7",
    "expo-image-picker": "~17.0.7",
    "expo-linear-gradient": "~15.0.6",
    "expo-linking": "~8.0.7",
    "expo-media-library": "~18.1.0",
    "expo-notifications": "~0.32.10",
    "expo-router": "~6.0.0-preview.16",
    "expo-status-bar": "~3.0.7",
    "expo-web-browser": "~15.0.6",
    "firebase": "^12.2.1",
    "hono": "^4.9.6",
    "mongodb": "^6.19.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-iap": "^14.1.0",
    "react-native-purchases": "^9.4.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "0.21.1",
    "react-native-worklets": "~0.5.0",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.12",
    "babel-jest": "^30.1.2",
    "expo-build-properties": "~1.0.7",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/backend/",
      "<rootDir>/.eas-inspect",
      "<rootDir>/.eas-inspect-pre"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|expo-.*|@expo/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-native-reanimated)/)"
    ]
  }
}
